<% provide(:title, 'Team index') %>

<h1>All Teams</h1>

<div class="row">
  <div class="col-sm-6">
    <div class="panel panel-primary">
      <div class="panel-header">List of All Teams</div>
      <div class="panel-body">
        <%= will_paginate %>
        <table class="table table-striped table-condensed table-hover">
          <tbody>
            <%= render @teams %>
          </tbody>
        </table>
        <%= will_paginate %>
      </div>
    </div>
  </div>
</div>
